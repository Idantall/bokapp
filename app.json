{
  "expo": {
    "name": "UP!",
    "slug": "up-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#FF9966"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.upapp.wellness"
    },
    "android": {
      "package": "com.upapp.wellness"
    },
    "scheme": "upapp",
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
